{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/swagger.json",
      "dest": "/public/swagger.json"
    }
  ],
  "buildCommand": "npm run generate-swagger && vercel build"
}
